											<a href="{{this.link}}" target="_blank" rel="nofollow noopener noreferrer"><img src="/img/icons/{{this.icon}}" alt="{{this.name}}" width="25"></a>
											{{/each}}
										</td>
									</tr>
								{{/or}}
							{{/each}}
						</tbody>
					</table>
				</div>
			</section>
    </div>
</main>
