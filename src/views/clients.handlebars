						<thead>
							<tr>
								<th>Name</th>
								{{#or isEthereum isGnosis}}
								{{else}}
									<th>Chains</th>
								{{/or}}
								<th>Language</th>
								<th>Prerequisity</th>
								<th>OS</th>
								<th>Latest version</th>
								<th>Social Media</th>
							</tr>
						</thead>
						<tbody>
							{{#each executionClients_arr}}
							{{#or ../isEthereum ../isGnosis}}
